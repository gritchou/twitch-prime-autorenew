    "bugs": {
        "url": "https://github.com/gritchou/twitch-prime-autorenew/issues"
    },
    "homepage": "https://github.com/gritchou/twitch-prime-autorenew#readme",
    "dependencies": {
        "webextension-toolbox": "^3.0.0"
    },
    "devDependencies": {
        "web-ext-types": "^3.2.1"
    }
}
