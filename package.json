{
    "name": "twitch-prime-autorenew",
    "version": "0.0.1",
    "description": "Twitch Prime Autorenew is a browser extension allowing user to renew twitch prime subscription with one click.",
    "scripts": {
        "start": "webextension-toolbox dev chrome",
        "build": "npm run -s build:chrome && npm run -s build:edge && npm run -s build:firefox",
        "build:chrome": "webextension-toolbox build chrome",
        "build:edge": "webextension-toolbox build edge",
        "build:firefox": "webextension-toolbox build firefox"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/gritchou/twitch-prime-autorenew.git"
    },
    "author": "Jean-Fran√ßois Grand",
